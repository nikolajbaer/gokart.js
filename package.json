{
  "name": "rect-web-game-starter",
  "version": "1.0.0",
  "description": "A starting point for a web game basedon ECSY, Three.js, Cannon-ES with React HUD/UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolajbaer/rect-web-game-starter.git"
  },
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --config ./webpack.prod.js",
    "start": "webpack serve --open --config ./webpack.dev.js --host 0.0.0.0",
    "test": "jest"
  },
  "author": "Nikolaj Baer",
  "license": "MIT",
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "cannon-es": "^0.16.0",
    "css-loader": "^5.1.3",
    "ecsy": "^0.4.2",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "three": ">=0.125.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "postcss-loader": "^5.2.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
